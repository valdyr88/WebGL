<assembly id="sovereign">

	<param bindpoint="scale" value="0.1"></param>
	<param bindpoint="position" value="0.0,0.0,0.0"></param> 

	<img id="engineering_Bc" src="./Textures/engineering_diff.png" type="tx2D/D" style="display: none;"></img>
	<img id="engineering_AoRS" src="./Textures/engineering_AoRSEm.png" type="tx2D/AoRS" style="display: none;"></img>
	<img id="engineering_N" src="./Textures/engineering_normal.png" type="tx2D/N" style="display: none;"></img>

	<img id="bottom_saucer_Bc" src="./Textures/bottom_saucer_diff.png" type="tx2D/D" style="display: none;"></img>
	<img id="bottom_saucer_AoRS" src="./Textures/bottom_saucer_AoRSEm.png" type="tx2D/AoRS" style="display: none;"></img>
	<img id="bottom_saucer_N" src="./Textures/bottom_saucer_normal.png" type="tx2D/N" style="display: none;"></img>

	<img id="nacelles_Bc" src="./Textures/nacelles_diff.png" type="tx2D/D" style="display: none;"></img>
	<img id="nacelles_AoRS" src="./Textures/nacelles_AoRSEm.png" type="tx2D/AoRS" style="display: none;"></img>
	<img id="nacelles_N" src="./Textures/nacelles_normal.png" type="tx2D/N" style="display: none;"></img>

	<img id="phasers_Bc" src="./Textures/phasers_diff.png" type="tx2D/D" style="display: none;"></img>
	<img id="phasers_AoRS" src="./Textures/phasers_AoRSEm.png" type="tx2D/AoRS" style="display: none;"></img>
	<img id="phasers_N" src="./Textures/phasers_normal.png" type="tx2D/N" style="display: none;"></img>

	<img id="top_saucer_Bc" src="./Textures/top_saucer_diff.png" type="tx2D/D" style="display: none;"></img>
	<img id="top_saucer_AoRS" src="./Textures/top_saucer_AoRSEm.png" type="tx2D/AoRS" style="display: none;"></img>
	<img id="top_saucer_N" src="./Textures/top_saucer_normal.png" type="tx2D/N" style="display: none;"></img>
	
	<model id="hull" src="./hull.obj" type="model/obj">
		<material shader="deferred_BcNAoRSMt">
			<param bindpoint="emissionMult" value="1.0" type="float"></param>
			<param bindpoint="roughnessScaleOffsetPower" value="0.5,0.5,1.0" type="vec3"></param>
			<texture bindpoint="txDiffuse" txid="engineering_Bc" type="tx2D/D"></texture>
			<texture bindpoint="txAoRS" txid="engineering_AoRS" type="tx2D/AoRS"></texture>
			<texture bindpoint="txNormal" txid="engineering_N" type="tx2D/N"></texture>
		</material>
	</model>
	
	<model id="bottom_saucer" src="./bottom_saucer.obj" type="model/obj">
		<material shader="deferred_BcNAoRSMt">
			<param bindpoint="emissionMult" value="1.0" type="float"></param>
			<param bindpoint="roughnessScaleOffsetPower" value="0.5,0.5,1.0" type="vec3"></param>
			<texture bindpoint="txDiffuse" txid="bottom_saucer_Bc" type="tx2D/D"></texture>
			<texture bindpoint="txAoRS" txid="bottom_saucer_AoRS" type="tx2D/AoRS"></texture>
			<texture bindpoint="txNormal" txid="bottom_saucer_N" type="tx2D/N"></texture>
		</material>
	</model>
	
	<model id="nacelles" src="./nacelles.obj" type="model/obj">
		<material shader="deferred_BcNAoRSMt">
			<param bindpoint="emissionMult" value="1.0" type="float"></param>
			<param bindpoint="roughnessScaleOffsetPower" value="0.5,0.5,1.0" type="vec3"></param>
			<texture bindpoint="txDiffuse" txid="nacelles_Bc" type="tx2D/D"></texture>
			<texture bindpoint="txAoRS" txid="nacelles_AoRS" type="tx2D/AoRS"></texture>
			<texture bindpoint="txNormal" txid="nacelles_N" type="tx2D/N"></texture>
		</material>
	</model>
	
	<model id="phasers" src="./phasers.obj" type="model/obj">
		<material shader="deferred_BcNAoRSMt">
			<param bindpoint="emissionMult" value="1.0" type="float"></param>
			<param bindpoint="roughnessScaleOffsetPower" value="0.5,0.5,1.0" type="vec3"></param>
			<texture bindpoint="txDiffuse" txid="phasers_Bc" type="tx2D/D"></texture>
			<texture bindpoint="txAoRS" txid="phasers_AoRS" type="tx2D/AoRS"></texture>
			<texture bindpoint="txNormal" txid="phasers_N" type="tx2D/N"></texture>
		</material>
	</model>
	
	<model id="top_saucer" src="./top_saucer.obj" type="model/obj">
		<material shader="deferred_BcNAoRSMt">
			<param bindpoint="emissionMult" value="1.0" type="float"></param>
			<param bindpoint="roughnessScaleOffsetPower" value="0.5,0.5,1.0" type="vec3"></param>
			<texture bindpoint="txDiffuse" txid="top_saucer_Bc" type="tx2D/D"></texture>
			<texture bindpoint="txAoRS" txid="top_saucer_AoRS" type="tx2D/AoRS"></texture>
			<texture bindpoint="txNormal" txid="top_saucer_N" type="tx2D/N"></texture>
		</material>
	</model>
	
	<model id="warp_grille" src="./warp_grille.obj" type="model/obj">
		<material shader="deferred_BcNAoRSMt">
			<param bindpoint="emissionMult" value="7.5" type="float"></param>
			<param bindpoint="roughnessScaleOffsetPower" value="0.5,0.5,1.0" type="vec3"></param>
			<texture bindpoint="txDiffuse" txid="bottom_saucer_Bc" type="tx2D/D"></texture>
			<texture bindpoint="txAoRS" txid="bottom_saucer_AoRS" type="tx2D/AoRS"></texture>
			<texture bindpoint="txNormal" txid="bottom_saucer_N" type="tx2D/N"></texture>
		</material>
	</model>
	
</assembly>